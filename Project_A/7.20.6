#include <iostream>
#include <vector>
#include <string>
#include <algorithm>

using namespace std;


int main() {
	string input;
	char ch;
	vector<char*> lines;
	
	while (cin.get(ch)) {
		if (ch == ' ') {	//add this to make sure the program worked rather than doing thousands characters
			break;
		}
		input += ch;
	}

	char* buffer = &input[0];

	lines.push_back(buffer);

	for (int i = 0; i < input.size(); i++) {
		if (input[i] == '\n') {
			input[i] = '\0';
			if (i + 1 < input.size()) {
				lines.push_back(&input[i + 1]);
			}
		}
	}
	cout << "Orginal order:\n";

	for (char* line : lines) {
		cout << line << endl;
	}

	sort(lines.begin(), lines.end(), [](char* a, char* b) {
		while (*a && *b) {
			if (*a < *b) {
				return true;
			}
			else if (*a > *b) {
				return false;
			}
			a++;
			b++;
		}
		return *a == '\0' && *b != '\0';
	});

	cout << endl;

	cout << "Sorted order:\n";
	
	for (char* line : lines) {
		cout << line << endl;
	}
	

	return 0;
}
